{
  "name": "eppo/php-sdk",
  "description": "Eppo PHP SDK",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/Eppo-exp/php-sdk",
  "authors": [
    {
      "name": "Pavlo Khrebto",
      "email": "pavel@fluxon.com"
    }
  ],
  "minimum-stability": "stable",
  "version": "1.0.0",
  "config": {
    "platform": {
      "php": "7.3.31"
    }
  },
  "autoload": {
    "psr-4": {
      "Eppo\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Eppo\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.3.31 || ^8.0",
    "guzzlehttp/guzzle": "^7.0",
    "ext-apcu": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6"
  }
}
